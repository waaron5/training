{
  "name": "fhss-starter-app",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "ng serve --host=0.0.0.0",
    "full-reload": "ng cache clean && npm ci && npm run prisma:generate && npm run dev",
    "build": "ng build",
    "build:prod": "npm run prisma:generate && ng build --configuration=production",
    "start:prod": "node dist/server/server.mjs",
    "prisma:generate": "prisma generate --no-hints",
    "prisma:studio": "prisma studio",
    "prisma:push": "prisma db push",
    "prisma:draft": "prisma migrate dev --create-only",
    "prisma:migrate": "prisma migrate deploy",
    "prisma:seed": "prisma db seed --",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "e2e": "npm run build:prod && playwright test",
    "e2e:ui": "npm run build:prod && playwright test --ui",
    "e2e:codegen": "npx playwright codegen --viewport-size='1920,1080' localhost:4200",
    "format:check": "prettier . --check",
    "format:fix": "prettier . --check --write ",
    "lint:check": "ng lint",
    "lint:fix": "ng lint --fix && npm run lint:ng",
    "lint:ng": "ng generate @angular/core:cleanup-unused-imports && ng generate @angular/core:self-closing-tag --defaults",
    "check": "npm run format:check && npm run lint:check && npm run test && npm run e2e && printf \"\\n\\n\\n\\033[1;32mYour code passed the check!\\033[0m\\nFeel free to commit.\\n\"",
    "check:packages": "npm audit && npm outdated"
  },
  "private": true,
  "dependencies": {
    "@angular/cdk": "^21.0.3",
    "@angular/common": "^21.0.3",
    "@angular/compiler": "^21.0.3",
    "@angular/core": "^21.0.3",
    "@angular/forms": "^21.0.3",
    "@angular/material": "^21.0.3",
    "@angular/platform-browser": "^21.0.3",
    "@angular/platform-browser-dynamic": "^21.0.3",
    "@angular/platform-server": "^21.0.3",
    "@angular/router": "^21.0.3",
    "@angular/ssr": "^21.0.3",
    "@faker-js/faker": "^10.1.0",
    "@fastify/cookie": "^11.0.2",
    "@fastify/formbody": "^8.0.2",
    "@fastify/static": "^8.2.0",
    "@fhss-web-team/backend-utils": "^21.0.2",
    "@fhss-web-team/frontend-utils": "^21.3.0",
    "@node-saml/node-saml": "^5.0.1",
    "@prisma/adapter-pg": "^7.1.0",
    "@prisma/client": "^7.1.0",
    "@trpc/client": "^11.4.2",
    "@trpc/server": "^11.4.2",
    "cron": "^4.3.2",
    "dotenv": "^17.2.3",
    "fastify": "^5.4.0",
    "jose": "^6.0.11",
    "ngx-quill": "^30.0.1",
    "nodemailer": "^7.0.11",
    "rxjs": "~7.8.0",
    "superjson": "^2.2.2",
    "tslib": "^2.3.0",
    "zod": "4.1.12"
  },
  "devDependencies": {
    "@analogjs/vite-plugin-angular": "^2.2.1",
    "@analogjs/vitest-angular": "^2.2.1",
    "@angular/build": "^21.0.3",
    "@angular/cli": "^21.0.3",
    "@angular/compiler-cli": "^21.0.3",
    "@playwright/test": "^1.57.0",
    "@types/node": "^24.10.3",
    "@types/nodemailer": "^7.0.4",
    "@vitest/coverage-istanbul": "^4.0.15",
    "@vitest/ui": "^4.0.15",
    "angular-eslint": "^21.1.0",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.3",
    "eslint-plugin-unused-imports": "^4.2.0",
    "jsdom": "^27.4.0",
    "playwright-ng-schematics": "^21.0.2",
    "prettier": "^3.6.2",
    "prisma": "^7.1.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.49.0",
    "vite": "^7.2.7",
    "vitest": "^4.0.15"
  }
}
